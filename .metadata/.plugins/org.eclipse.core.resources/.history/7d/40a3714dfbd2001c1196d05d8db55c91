package br.com.br.ney.belchiofinancas.domain.pessoas;

public enum TipoPessoa {
	
	FISICA(1),
	JURIDICA(2);
	
   

	Integer valor;
	  
	TipoPessoa(int pessoa) {
			this.valor=pessoa;
		}

	
	public int getValor() {
		return valor;
	}
	
	static TipoPessoa toEnum(int valor) {
	
	     TipoPessoa[] values = TipoPessoa.values();
	     
		
		}

	}
	  
	  
}
